{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-texas-south-dev"
		},
		"ADLS1_DEV_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS1_DEV'"
		},
		"ADLS2_DEV_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS2_DEV'"
		},
		"ADLS1_DEV_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagedevgit.dfs.core.windows.net/"
		},
		"ADLS2_DEV_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagetestingrgit.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-texas-south-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS1_DEV_accountKey": {
						"value": "[parameters('ADLS1_DEV_accountKey')]"
					},
					"ADLS2_DEV_accountKey": {
						"value": "[parameters('ADLS2_DEV_accountKey')]"
					},
					"ADLS1_DEV_properties_typeProperties_url": {
						"value": "[parameters('ADLS1_DEV_properties_typeProperties_url')]"
					},
					"ADLS2_DEV_properties_typeProperties_url": {
						"value": "[parameters('ADLS2_DEV_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}